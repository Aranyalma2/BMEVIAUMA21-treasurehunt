# Docker Compose Environment Configuration
# Copy this file to .env and fill in your values

# Domain Configuration
DOMAIN=example.com

# Database Configuration
POSTGRES_DB=treasurehunt
POSTGRES_USER=treasurehunt
POSTGRES_PASSWORD=change-this-secure-password

# Backend Configuration
JWT_SECRET=change-this-to-a-secure-random-string
CORS_ORIGIN=https://example.com,https://admin.example.com

# Frontend/Admin Build Configuration
# This should point to your domain (used at build time)
VITE_BACKEND_URL=https://example.com/api
